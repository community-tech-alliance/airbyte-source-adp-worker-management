      },
      "ssl_cert": {
        "title": "SSL Certificate",
        "description": "",
        "type": "string",
        "airbyte_secret": "true",
        "multiline": true,
        "order": 3
      },
      "ssl_key": {
        "title": "SSL Key",
        "description": "",
